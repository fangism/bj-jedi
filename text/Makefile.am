CLEANFILES = $(GENERATED_TABLES) $(GENERATED_DOT) $(GENERATED_FIGS)

tables: $(GENERATED_TABLES)
dots: $(GENERATED_DOT)
figs: $(GENERATED_FIGS)
clean-local: clean-tables clean-dot clean-figs
clean-tables:
	rm -f $(GENERATED_TABLES)
clean-dot:
	rm -f $(GENERATED_DOT)
clean-figs:
