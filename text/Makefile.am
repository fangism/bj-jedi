	$(SED) -n '/Player.*hit transition/,/^$$/p' $< | \
		$(SED) -e '1d' -e '$$d' | \
		$(SM_TO_TABULAR) ; \
	$(END_TABULAR) ;} > $@

tables/player-final-split.tex: $(BASIC_STRATEGY)
	{ $(TITLE_COMMENT) ; \
	$(NEXT_CARD_TABULAR) ; \
	$(HOME_NEXT_CARD_PLAYER) ; \
	$(NEXT_CARD_HEADER) ; \
	{ $(SED) -n '/Player.*hit transition/,/^$$/p' $< | \
		$(SED) -e '1d' -e '/^35/,$$d' ; \
	$(SED) -n '/Player.*split.*final.*transition/,/^$$/p' $< | \
		$(SED) -e '1d' -e '$$d' | \
		$(AWK) '{print $$1+35 "\t" $$2 "\t" $$3;}' ;} | \
		$(SM_TO_TABULAR) | $(TAIL) -n 10 ; \
	$(END_TABULAR) ;} > $@

