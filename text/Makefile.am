SUFFIXES += .dealer-odds .dealer-final
.dealer-odds.dealer-final:
	soft17=`grep "soft 17" $< | cut -d\  -f4` && \
	p=`grep "peek on A" $< | cut -d\  -f5` && \
	case $$soft17 in \
	hits) var="H17" ;; \
	stands) var="S17" ;; \
	esac && \
	case $$p in \
	yes) peek="post" ;; \
	no) peek="pre" ;; \
	esac && \
	$(SED) -n -e '/^dealer.*bust.*push/,$$p' $< | \
	$(SED) -e '/^dealer/s|dealer|&	peek	soft-17|' \
		-e "/^[2-9TA]/s|^.|&	$$peek	$$var|" | \
	$(SED) -e '/^[2-9]/s:p[a-z]*::' \
		-e '/^[7-9T]/s:[HS]17::' > $@

# combines 4 tables into 1
# predefine: fpreh17, fposth17, fpres17, fposts17
DEALER_FINAL_UNIFY_SCRIPT = \
	{ $(SED) -n -e '/^dealer/,/^6/p' $$fposth17 ; \
	$(SED) -n -e '/^2/,/^T/p' $$fposts17 ; \
	$(GREP) "^T" $$fpreh17 ; \
	$(GREP) "^A" $$fposth17 ; \
	$(GREP) "^A" $$fpreh17 ; \
	$(GREP) "^A" $$fposts17 ; \
	$(GREP) "^A" $$fpres17  ;}

tables/dealer/dealer-final-basic.dealer-final-unified: \
		tables/dealer/H17-pre-peek-basic.dealer-final \
		tables/dealer/H17-post-peek-basic.dealer-final \
		tables/dealer/S17-pre-peek-basic.dealer-final \
		tables/dealer/S17-post-peek-basic.dealer-final
	fpreh17=$< ; \
	fposth17=tables/dealer/H17-post-peek-basic.dealer-final ; \
	fpres17=tables/dealer/S17-pre-peek-basic.dealer-final ; \
	fposts17=tables/dealer/S17-post-peek-basic.dealer-final ; \
	$(DEALER_FINAL_UNIFY_SCRIPT) > $@

