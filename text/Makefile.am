pdfdir = $(docdir)/pdf
dist_pdf_DATA = bj-math.pdf

###############################################################################
# dot -> fig support (graphviz)
SUFFIXES += .dot .fig
.dot.fig:
	$(DOT) -Tfig $< -o $@

###############################################################################
BOOK_INPUTS = bj-math.tex \
	preface.tex \
	intro.tex \
	dealer.tex \
	basic.tex \
	composition.tex \
	counting.tex \
	betting.tex \
	variations.tex \
	experimental.tex \
